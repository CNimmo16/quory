{
  "name": "@quory/stack",
  "version": "0.1.0",
  "description": "Integrate Quory into your existing web app",
  "type": "module",
  "exports": {
    "./server": {
      "import": "./dist/server/index.js",
      "types": "./dist/server/index.d.ts"
    },
    "./react": {
      "import": "./dist/react/index.js",
      "types": "./dist/react/index.d.ts"
    },
    "./react/styles.css": {
      "import": "./dist/react/styles.css",
      "require": "./dist/react/styles.css"
    }
  },
  "author": "CNimmo16",
  "license": "MIT",
  "scripts": {
    "test": "jest --runInBand",
    "build": "tsup --entry src/server/index.ts --entry src/react/index.ts --format cjs,esm --dts",
    "build:watch": "npm run build -- --watch --sourcemap",
    "lint": "eslint ."
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@quory/config": "workspace:*",
    "@types/better-sqlite3": "^7.6.12",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@types/string-similarity": "^4.0.2",
    "better-sqlite3": "^11.7.0",
    "jest": "^29.7.0",
    "react-dom": "^19.0.0",
    "ts-jest": "^29.2.5",
    "tsup": "^8.3.5"
  },
  "dependencies": {
    "@quory/core": "workspace:*",
    "@quory/postgres": "workspace:*",
    "@quory/sqlite": "workspace:*",
    "@tanstack/react-query": "^5.62.10",
    "@trpc/client": "^11.0.0-rc.666",
    "@trpc/server": "^11.0.0-rc.666",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "immer": "^10.1.1",
    "knex": "^2.3.0",
    "react": "^19.0.0",
    "react-icons": "^5.4.0",
    "string-similarity": "^4.0.4",
    "testcontainers": "^8.14.0",
    "zod": "^3.24.1"
  }
}