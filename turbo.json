{
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },
    "build:watch": {
      "dependsOn": [
        "^build:watch"
      ],
      "cache": false,
      "persistent": true
    },
    "serve": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "dependsOn": [
        "^build"
      ],
      "cache": false
    }
  }
}